<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Qlik Plateform Manager - @ViewBag.Title</title>
    @*    @Styles.Render("~/Content/css")*@
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="~/Content/mine.css" />
    @Scripts.Render("~/bundles/modernizr")
    <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate-vsdoc.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/mine/utils.js")" type="text/javascript"></script>

    @*<script src="~/Scripts/jquery.validate-vsdoc.js"></script>
        <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>*@

</head>
<body>

    
    <div class="container-fluid">

        @*HEADER*@
        <header class="row">
            <div class="container-fluid">
                <nav class="navbar navbar-expand-lg navbar-light bg-light">
                    <a class="navbar-brand" href='@Url.Action("Index", "Afficher")'>
                        <img src="~/Content/Pictures/Qlik_icon.ico" width="30" height="30" />
                        Platform Manager &nbsp;&nbsp;&nbsp;&nbsp;
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav mr-auto">
                            @* Administration *@
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span class="fas fa-tools"></span>
                                    Administration
                                    <span class="caret"></span>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    @Html.ActionLink("Utilisateurs", "Utilisateur", "Administrer", new { @class = "dropdown-item" })
                                    @Html.ActionLink("Menus", "Menu", "Administrer", new { @class = "dropdown-item" })
                                    @*<div class="dropdown-divider"></div>*@
                                    @*<a class="dropdown-item" href="#">Something else here</a>*@
                                </div>
                            </li>
                            @* Rapport *@
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span class="fas fa-file-alt"></span>
                                    Rapport
                                    <span class="caret"></span>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    @Html.ActionLink("Activité développeurs", "Activite", "Analyser", new { @class = "dropdown-item" })
                                    @Html.ActionLink("Exécution Tâches", "Tache", "Analyser", new { @class = "dropdown-item" })
                                </div>
                            </li>
                        </ul>
                        <span class="navbar-text">
                            Bonjour, @User.Identity.Name!
                        </span>
                        @*<form class="form-inline my-2 my-lg-0">
                            <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                            <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
                            </form>*@
                    </div>
                </nav>
            </div>
        </header>
        <div class="row"><br /></div>
        @*ZONE CENTRALE*@
        <div class="row">

            @*MENU*@
            <nav class="col-sm-2">
                <div class="list-group">
                    <div class="list-group-item list-group-item-dark">
                        <h6 class="list-group-item-heading "><span class="fas fa-location-arrow"></span> DEPLOIEMENT</h6>
                    </div>
                    @Html.ActionLink("Script", "Script", "Deployer", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Application", "Application", "Deployer", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Package", "Package", "Deployer", null, new { @class = "list-group-item list-group-item-action" })
                    <div class="list-group-item list-group-item-dark">
                        <h6 class="list-group-item-heading "><span class="fas fa-archive"></span> ARCHIVAGE</h6>
                    </div>
                    @Html.ActionLink("Application", "Application", "Archiver", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Structure", "Structure", "Archiver", null, new { @class = "list-group-item list-group-item-action" })
                    <div class="list-group-item list-group-item-dark">
                        <h6 class="list-group-item-heading "><span class="fas fa-calendar-day"></span> PLANIFICATION</h6>
                    </div>
                    @Html.ActionLink("Deploiement", "Deploiement", "Planifier", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Archivage", "Archivage", "Planifier", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Test", "Test", "Planifier", null, new { @class = "list-group-item list-group-item-action" })
                    <div class="list-group-item list-group-item-dark">
                        <h6 class="list-group-item-heading "><span class="fas fa-edit"></span> MISE A JOUR</h6>
                    </div>
                    @Html.ActionLink("Dimension & Mesure", "Dimension", "MAJ", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Structure", "Structure", "MAJ", null, new { @class = "list-group-item list-group-item-action" })
                    <div class="list-group-item list-group-item-dark">
                        <h6 class="list-group-item-heading "><span class="fas fa-eye"></span> SUPERVISION</h6>
                    </div>
                    @Html.ActionLink("Session", "Session", "Superviser", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Performance", "Performance", "Superviser", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Service", "Service", "Superviser", null, new { @class = "list-group-item list-group-item-action" })
                    <div class="list-group-item list-group-item-dark">
                        <h6 class="list-group-item-heading "><span class="fas fa-check-double"></span> TEST</h6>
                    </div>
                    @Html.ActionLink("Sécurité", "Securite", "Tester", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Réduction de données", "Reduction", "Tester", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Valeur objet", "Donnees", "Tester", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Résultats", "Resultats", "Tester", null, new { @class = "list-group-item list-group-item-action" })
                    <div class="list-group-item list-group-item-dark">
                        <h6 class="list-group-item-heading "><span class="fas fa-info"></span> INFOS</h6>
                    </div>
                    @Html.ActionLink("Documentation", "Documentation", "Afficher", null, new { @class = "list-group-item list-group-item-action" })
                    @Html.ActionLink("Liens", "Liens", "Afficher", null, new { @class = "list-group-item list-group-item-action" })
                </div>
            </nav>

            @*CONTENU*@
            <main class="col-sm-10">
                <div class="row">
                    <div class="container">
                        @*<div class="col-sm-12 colonne-centree">*@
                        @RenderBody()
                        @*</div>*@
                    </div>
                </div>
            </main>
        </div>

        @*FOOTER*@
        <footer class="footer">
            <div class="container-fluid" style="text-align:right">
                <hr />
                <span class="text-muted">© SOFIREP</span>
            </div>
        </footer>
    </div>


    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    @*<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>*@



    @*    @Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
